{
  "name": "@men-mvc/filesystem",
  "version": "0.89.0",
  "description": "Package that includes shared variables, types and utility functions for MEN MVC framework.",
  "private": false,
  "type": "commonjs",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/men-mvc/dependencies.git"
  },
  "scripts": {
    "build": "rm -f -r lib && npx tsc",
    "dev": "echo \"dev command is not required for test package\"",
    "format": "prettier --config .prettierrc . --write",
    "test": "CORE_TEST=1 jest --testPathPattern=tests --runInBand --forceExit",
    "jest": "CORE_TEST=1 jest"
  },
  "dependencies": {
    "@men-mvc/foundation": "^0.89.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.16",
    "@types/sinon": "^10.0.13",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "sinon": "^15.0.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "author": "Wai Yan Hein <waiyanhein.uk@gmail.com>",
  "license": "MIT"
}
