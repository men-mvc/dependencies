{
  "server": {
    "port": "SERVER_PORT"
  },
  "auth": {
    "secret": "AUTH_TOKEN_SECRET_KEY"
  },
  "mail": {
    "driver": "MAIL_DRIVER",
    "nodemailer": {
      "user": "MAIL_USER",
      "password": "MAIL_PASSWORD",
      "authType": "MAIL_AUTH_TYPE"
    }
  },
  "fileSystem": {
    "storageDriver": "FILESYSTEM_STORAGE_DRIVER",
    "s3": {
      "region": "AWS_S3_REGION",
      "accessKeyId": "AWS_S3_ACCESS_KEY_ID",
      "secretAccessKey": "AWS_S3_SECRET_ACCESS_KEY",
      "bucket": "AWS_S3_BUCKET_NAME"
    }
  },
  "cache": {
    "driver": "CACHE_DRIVER"
  },
  "envVariables": {
    "stringVar": {
      "name": "STRING_VAR",
      "type": "string"
    },
    "numberVar": {
      "name": "NUMBER_VAR",
      "type": "number"
    },
    "stringArrayVar": {
      "name": "STRING_ARRAY_VAR",
      "type": "string_array"
    },
    "numberArrayVar": {
      "name": "NUMBER_ARRAY_VAR",
      "type": "number_array"
    },
    "booleanVarStringTrue": {
      "name": "BOOLEAN_VAR_STRING_TRUE",
      "type": "boolean"
    },
    "booleanVarStringFalse": {
      "name": "BOOLEAN_VAR_STRING_FALSE",
      "type": "boolean"
    },
    "booleanVarNumberTrue": {
      "name": "BOOLEAN_VAR_NUMBER_TRUE",
      "type": "boolean"
    },
    "booleanVarNumberFalse": {
      "name": "BOOLEAN_VAR_NUMBER_FALSE",
      "type": "boolean"
    },
    "nested": {
      "stringVar": {
        "name": "NESTED_STRING_VAR",
        "type": "string"
      },
      "numberVar": {
        "name": "NESTED_NUMBER_VAR",
        "type": "number"
      },
      "stringArrayVar": {
        "name": "NESTED_STRING_ARRAY_VAR",
        "type": "string_array"
      },
      "numberArrayVar": {
        "name": "NESTED_NUMBER_ARRAY_VAR",
        "type": "number_array"
      },
      "booleanVar": {
        "name": "NESTED_BOOLEAN_VAR",
        "type": "boolean"
      }
    }
  },
  "invalidEnvVariables": {
    "nameIsEmpty": {
      "name": "",
      "type": "string"
    },
    "typeIsUndefined": {
      "name": "TYPE_IS_UNDEFINED"
    },
    "invalidTypeValue": {
      "name": "INVALID_TYPE_VALUE",
      "type": "invalid-type-value"
    }
  }
}
